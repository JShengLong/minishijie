/* pages/set/feedback.wxss */
@import '../set/modifyph';

/*选择图片View*/

.addImv {
    margin: 20rpx 0 0 0;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
}

/*添加图片*/

.addImv .chooseView {
    width: 200rpx;
    height: 200rpx;
    text-align: center;
    padding: 7rpx;
}

.addImv .chooseImv {
    width: 200rpx;
    height: 200rpx;
    padding: 7rpx;
}

/*已选择的图片*/

.addImv .upFile {
    width: 200rpx;
    height: 200rpx;
    position: relative;
    padding: 7rpx;
}

.addImv .upFile .itemImv {
    width: 200rpx;
    height: 200rpx;
    padding: 7rpx;
}

.addImv .upFile .closeImv {
    position: absolute;
    right: 0rpx;
    top: 0rpx;
    width: 30rpx;
    height: 30rpx;
}

.feedback {
    padding: 3%;
}
