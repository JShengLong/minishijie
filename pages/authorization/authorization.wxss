.container{
    padding-top: 200rpx;
}
.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
 
.header image {
    width: 200rpx;
    height: 200rpx;
}
 
.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}
 
.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.login_bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
}
button{
    width: 90% !important;
    margin: auto;
    height: 80rpx;
    line-height: 80rpx !important;
}